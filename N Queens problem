#include <stdio.h>
int count = 0;
int isSafe(int board[][10], int row, int col, int n) {
	for (int i = 0; i < row; i++) {
		if (board[i][col] == 1) {
			return 0;
		}
	}
	for (int i = row - 1, j = col - 1; i >= 0 && j >= 0; i--, j--) {
		if (board[i][j] == 1) {
			return 0;
		}
	}
	for (int i = row - 1, j = col + 1; i >= 0 && j < n; i--, j++) {
		if (board[i][j] == 1) {
			return 0;
		}
	}
	return 1;
}

void solveNQueens(int board[][10], int row, int n) {
	if (row == n) {
		count++;
		return;
	}
	for (int col = 0; col < n; col++) {
		if (isSafe(board, row, col, n)) {
			board[row][col] = 1;
			solveNQueens(board, row + 1, n);
			board[row][col] = 0;
		}
	}
}

int nQueen(int n) {
	int board[10][10] = {0};
	count = 0;
	solveNQueens(board, 0, n);
	return count;
}

int main() {
	int n;
	scanf("%d", &n);
	printf("%d\n", nQueen(n));
	return 0;
}
